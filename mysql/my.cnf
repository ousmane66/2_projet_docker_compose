[client]
port            = 3306


[mysqld_safe]
user			= mysql
pid-filee		= /var/run/mysqld/mysqld.pid
socket		= /var/run/mysqld/mysqld.sock
port            = 3306
basedir		= /usr
datadir		= /var/lib/mysql
tmpdir		= /tmp
language		= /usr/share/mysql/french
skip-external-locking
bind-address    = 172.17.0.3

[mysqld]
user			= mysql
pid-filee		= /var/run/mysqld/mysqld.pid
socket		= /var/run/mysqld/mysqld.sock
port            = 3306
basedir		= /usr
datadir		= /var/lib/mysql
tmpdir		= /tmp
language		= /usr/share/mysql/french
skip-external-locking
bind-address    = 172.17.0.3
